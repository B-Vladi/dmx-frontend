{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "dist",
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "alwaysStrict": true,
    "strict": true,
    "sourceMap": true,
    "traceResolution": true,
    "plugins": [{
      "name": "@vue/babel-preset-app"
    }]
  },
  "extends": "@vue/tsconfig/tsconfig.json",
  "references": [
    {
      "path": "tsconfig.app.json"
    },
    {
      "path": "tsconfig.node.json"
    }
  ]
}

{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "composite": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "plugins": [{
      "name": "@vue/babel-preset-app"
    }],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    "./src/**/__tests__/*",
    "node_modules"
  ],
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "./src/**/*"
  ]
}
